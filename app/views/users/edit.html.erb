<%= form_for @user do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>

  <%= f.label :slack %>
  <%= f.text_field :slack %><br>

  <%= f.label :github %>
  <%= f.text_field :github %><br>

  <%= f.label :twitter %>
  <%= f.text_field :twitter %><br>

  <%= f.label :website %>
  <%= f.text_field :website %><br>

  <%= f.label :bio %>
  <%= f.text_field :bio %><br>

  <%= f.label :availability %>
  <%= f.text_field :availability %><br>

  <%= f.label :application %>
  <%= f.text_field :application %><br>
  
  <%= f.submit %>

<% end %>